<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,300i,400,400i,500,500i,600,600i,700,700i&amp;display=swap" rel="stylesheet">
    @using System.Web.Optimization
    @Styles.Render("~/bundles/bootstrap/css")
    @Styles.Render("~/bundles/bootstrap/awesome-fonts")
    @Styles.Render("~/bundles/bootstrap/owl")
    @Styles.Render("~/bundles/bootstrap/slicknav")
    @Styles.Render("~/bundles/bootstrap/stylecss")
</head>
<body>
    @{ Html.RenderPartial("Loading"); }
    @{ Html.RenderPartial("_HeaderNavBar"); }
    @RenderBody()
    @{ Html.RenderPartial("_PartialFooter"); }

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/slicknav")
    @Scripts.Render("~/bundles/owl")
    @Scripts.Render("~/bundles/mixitup")
    @Scripts.Render("~/bundles/main")
    


</body>
</html>